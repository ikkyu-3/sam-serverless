{
  "name": "sam-serverless",
  "version": "0.0.0",
  "description": "sta-access-manager-server × serverless × TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "tsc --noEmit && babel src --out-dir serverless --extensions .ts --source-maps inline",
    "deploy": "npm run build && cd serverless && serverless deploy -v && cd -",
    "remove": "cd serverless && serverless remove -v && cd -",
    "test": "jest --runInBand --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikkyu-3/sam-serverless.git"
  },
  "author": "ikkyu-3",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ikkyu-3/sam-serverless/issues"
  },
  "homepage": "https://github.com/ikkyu-3/sam-serverless#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/aws-lambda": "^8.10.19",
    "@types/dotenv": "^6.1.0",
    "@types/expect-puppeteer": "^3.3.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.0",
    "@types/jest-environment-puppeteer": "^2.2.1",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.24",
    "@types/puppeteer": "^1.12.1",
    "axios": "^0.18.0",
    "babel-jest": "^24.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-puppeteer": "3.9.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "puppeteer": "^1.12.2",
    "serverless": "^1.37.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.400.0",
    "lodash": "^4.17.11"
  }
}
