{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "prettier": [
        true,
        {
          "semi": true,
          "trailingComma": "es5"
        }
      ]
    },
    "rulesDirectory": ["tslint-plugin-prettier"],
    "linterOptions": {
      "exclude": [
        "severless"
      ]
    }
}